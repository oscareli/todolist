{
  "extends": "@istanbuljs/nyc-config-typescript",
  "all": true,
  "check-coverage": true,
  "branches": 80,
  "lines": 80,
  "functions": 80,
  "statements": 80,
  "excludeNodeModules": true,
  "include": ["src/**/*"],
  "exclude": ["specs/**/*.spec.ts", "src/index.ts", "src/app.ts"],
  "reporter": ["text", "lcov"]
}
