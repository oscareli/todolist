{
  "name": "todolist",
  "version": "1.0.0",
  "description": "API que forne√ße uma lista de todo/tasks",
  "main": "build/index.js",
  "scripts": {
    "build": "tsc",
    "start": "NODE_ENV=production node ./build/index.js",
    "dev": "ts-node-dev --inspect=0.0.0.0 --respawn --debug --transpile-only  ./src/index.ts",
    "tslint": "tslint --project tsconfig.json",
    "test": "jasmine-ts --config=jasmine.json",
    "prettier:base": "prettier --parser typescript --single-quote",
    "prettier:check": "npm run prettier:base -- --list-different \"src/**/*.{ts,tsx}\"",
    "prettier:write": "npm run prettier:base -- --write \"src/**/*.{ts,tsx}\"",
    "cover": "nyc npm run test",
    "check": "npm run tslint && npm run cover && npm run prettier:check"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run prettier:write",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/oscareli/todolist.git"
  },
  "author": "Oscar Oliveira",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/oscareli/todolist/issues"
  },
  "homepage": "https://github.com/oscareli/todolist#readme",
  "devDependencies": {
    "@commitlint/cli": "^9.1.2",
    "@commitlint/config-conventional": "^10.0.0",
    "@istanbuljs/nyc-config-typescript": "^1.0.1",
    "@types/debug": "^4.1.5",
    "@types/express": "^4.17.7",
    "@types/jasmine": "^3.5.13",
    "husky": "^4.2.5",
    "jasmine": "^3.6.1",
    "jasmine-spec-reporter": "^5.0.2",
    "jasmine-ts": "^0.3.0",
    "nyc": "^15.1.0",
    "prettier": "^2.0.5",
    "ts-node-dev": "^1.0.0-pre.60",
    "tslint": "^6.1.3",
    "tslint-config-airbnb": "^5.11.2",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.0.2",
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.7",
    "@types/method-override": "0.0.31"
  },
  "dependencies": {
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "method-override": "^3.0.0"
  }
}
